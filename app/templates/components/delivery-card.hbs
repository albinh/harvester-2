{{#paper-card  class="flex-100 delivery-completed" as |card|}}
    {{#card.title as |title|}}
        {{#title.text as |text|}}
            {{#text.headline}}{{delivery.customer.name}}{{/text.headline}}
            {{#text.subhead}}{{delivery.shortDate}}{{/text.subhead}}
        {{/title.text}}

    {{/card.title}}
    {{#card.content}}
<md-chips class="md-contact-chips md-focused">
  <md-chips-wrap class="md-chips md-contact-chips  md-readonly" tabindex="-1" >

{{#each delivery.crops as |crop|}}

{{delivery-crop-chip dc=crop}}
{{/each}}
</md-chips-wrap>
</md-chips>

       


    {{/card.content}}
    {{#card.actions}}
        {{#paper-button href=(href-to "delivery" delivery)}}Visa detaljer{{/paper-button}}
    {{/card.actions}}
{{/paper-card}}